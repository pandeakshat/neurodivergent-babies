#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/pandeakshat/code/pandeakshat/neurodivergent-babies/core/node_modules/.pnpm/astro@5.8.1_@types+node@22.15.29_jiti@2.4.2_lightningcss@1.30.1_rollup@4.41.1_typescript@5.8.3/node_modules/astro/node_modules:/home/pandeakshat/code/pandeakshat/neurodivergent-babies/core/node_modules/.pnpm/astro@5.8.1_@types+node@22.15.29_jiti@2.4.2_lightningcss@1.30.1_rollup@4.41.1_typescript@5.8.3/node_modules:/home/pandeakshat/code/pandeakshat/neurodivergent-babies/core/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/pandeakshat/code/pandeakshat/neurodivergent-babies/core/node_modules/.pnpm/astro@5.8.1_@types+node@22.15.29_jiti@2.4.2_lightningcss@1.30.1_rollup@4.41.1_typescript@5.8.3/node_modules/astro/node_modules:/home/pandeakshat/code/pandeakshat/neurodivergent-babies/core/node_modules/.pnpm/astro@5.8.1_@types+node@22.15.29_jiti@2.4.2_lightningcss@1.30.1_rollup@4.41.1_typescript@5.8.3/node_modules:/home/pandeakshat/code/pandeakshat/neurodivergent-babies/core/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../astro.js" "$@"
else
  exec node  "$basedir/../../astro.js" "$@"
fi
